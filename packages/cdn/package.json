{
  "name": "@shutters/cdn",
  "type": "module",
  "module": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "typecheck": "tsc",
    "test": "bun test",
    "ci:codequality": "biome ci .",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "build": "bun build --compile --minify --sourcemap ./src/index.ts --outfile dist/cdn",
    "build:clean": "bun run clean && bun run build",
    "dev": "bun run ./src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@atcute/cid": "^2.1.0",
    "@aws-sdk/client-dynamodb": "^3.744.0",
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/lib-dynamodb": "^3.744.0",
    "@sentry/bun": "^9.1.0",
    "@shutters/shutterkit": "workspace:*",
    "hono": "^4.7.1",
    "prom-client": "^15.1.3"
  }
}
